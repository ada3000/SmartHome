<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <threads>
    <min worker="" io="" />
    <max worker="" io="" />
  </threads>
  <processes checkRunSecs="10">
    <process id="TelemetryPushProvider" name="TelemetryPushProvider" assembly="SH.Biz" class="SH.Biz.TelemetryPushProviderProcess">

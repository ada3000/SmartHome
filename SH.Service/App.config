<?xml version="1.0"?>
<configuration>
	<appSettings>
		<add key="ConfigFilePath" value="Service.Processes.xml"/>
		<!-- интервал опроса файла конфигурации -->
		<add key="CheckConfigTimeoutSec" value="60"/>
	</appSettings>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/>
	</startup>
	<system.net>
		<connectionManagement>
			<add address="*" maxconnection="1000"/>
		</connectionManagement>
	</system.net>
</configuration>